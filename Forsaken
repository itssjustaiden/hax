-- fartsaken (credits to ivan and other devs)
if getgenv then
    getgenv().BloxtrapRPC = "false"
    getgenv().DebugNotifications = "false"
    getgenv().TrackMePlease = "false"
end

loadstring(game:HttpGet("https://raw.githubusercontent.com/ivannetta/ShitScripts/main/forsaken.lua"))()

-- auto block (credits to me xd)
local StarterGui = game:GetService("StarterGui")
StarterGui:SetCore("SendNotification", {
	Title = "Fartsaken",
	Text = "Load guest auto-block?",
	Duration = 60,
	Button1 = "Yes",
	Button2 = "No",
	Callback = function(buttonPressed)
		if buttonPressed == "Yes" then
			loadstring(game:HttpGet('https://raw.githubusercontent.com/itssjustaiden/hax/main/ForsakenAutoBlock'))()
		else
			print("have fun using fartsaken!")
		end
	end
})
